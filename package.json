  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MazeMap/Leaflet.VisualClick.git"
  },
  "keywords": [
    "map",
    "leaflet",
    "click",
    "feedback"
  ],
  "author": "Dag Jomar <dagjomar@mazemap.no>",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/MazeMap/Leaflet.VisualClick/issues"
  },
  "dependencies": {
    "gobble": "^0.10.2",
    "gobble-autoprefixer": "^0.1.1",
    "gobble-cli": "^0.5.0",
    "gobble-zip": "^0.0.2"
  }
}
