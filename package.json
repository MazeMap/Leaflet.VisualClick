{
  "name": "Leaflet.VisualClick",
  "version": "1.1.0",
  "description": "Add visual feedback to touches/clicks on a LeafletJS map",
  "main": "dist/L.VisualClick.js",
  "scripts": {
    "build": "node_modules/gobble-cli/lib/index.js build -f dist",
    "postinstall": "npm run-script build",
    "prepublish": "npm run-script build",
    "watch": "node_modules/gobble-cli/lib/index.js watch -f dist",
    "start": "npm run-script watch",
